	<queryString>
		<![CDATA[SELECT pess.NM_PES NOME_PROPONENTE,
       pfis.DT_NAS DATA_NASCIMENTO,
       pess.NR_CIC NR_CPF,
       pfis.CD_EST_CVL ESTADO_CIVIL, --SOLTEIRO --CASADO --DIVORCIADO --VIUVO
       (select descricao
          from cup_tipo_documento tp
         where tp.ID_TIPO_DOC = pfis.ID_TIPO_DOC) NATUREZA_DOC_IDENTIFICACAO,
       pfis.NR_CAR_IDT NR_IDENTIFICACAO,
       pfis.SG_ORG_EMS ORGAO_EXPEDIDOR,
       pfis.DT_EMS_IDT DT_EXPEDICAO,
       ende.NM_LOG ENDERECO_RESIDENCIAL,
       ende.NR_EN NR_ENDERECO,
       ende.NM_BAI BAIRRO_ENDERECO,
       ende.DE_CPL_EN COMPLEMENTO_ENDERECO,
       ende.NM_CID CIDADE_ENDERECO,
       ende.CD_CEP CEP_ENDERECO,
       ende.SG_UF UF_ENDERECO,
       '(' || ende.NR_DDD || ') ' || ende.NR_FN || ' / (' ||
       ende.NR_DDD_CEL || ') ' || ende.NR_CEL TELEFONE_COM_DDD,
       (select DE_PRF
          from t400prof
         where cd_cbo = (select CD_CBO
                           from T400PFIS
                          where cd_cli = $P{codigoProponente})) PROFISSAO,
       cofi.sg_atv_ecn ATIVIDADE,
       (select sum(vr_pcp)
          from t423rdto
         where cd_sq_ori =
               (select cd_sq_ori
                  from t423cdto
                 where cd_cli = $P{codigoProponente})) RENDA_MENSAL,
       (select VR_PTR_LIQ from t423cdto where cd_cli = $P{codigoProponente}) PATRIMONIO,
       (select max(NR_APOLICE) from T402SESG where id_sit = 'A') NR_APOLICE
  FROM T400PESS pess, T400PFIS pfis, t402cofi cofi, t400ende ende
 where pess.cd_cli = pfis.cd_cli
   and pess.cd_cli = cofi.cd_cli
   and pess.cd_cli = cofi.cd_cli
   and pess.cd_cli = ende.cd_cli
   and pfis.cd_cli = ende.cd_cli
   and cofi.cd_und = ende.cd_und
   and cofi.cd_und = ende.cd_und
   and pess.cd_cli = $P{codigoProponente}
   and cofi.cd_und = $P{agenciaProponente}
   and cofi.cd_cc = $P{contaProponente}
   and rownum = 1]]>
	</queryString>
